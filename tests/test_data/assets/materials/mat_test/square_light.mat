pipelines:
flags:
    - two_sided
    - emissive
textures:
    - default_basecolor.tex
    - default_normalmap.dds
    - default_roughness.tex
    - default_metallic.tex
    - default_opacity.tex
    - mat_test/emissive_stripes.tex
params:
    - 0.0 0.0 0.0 0.5      # base color, roughness
    - 0.0 0.0 0.0 0.0      # sheen, sheen_tint, specular, spec_tint
    - 0.0 0.0 0.0 0.0      # metallic, transmission, clearcoat, clearcoat_roughness
    - 0.0 20.3718 20.3718 20.3718      # transparency, emission color