pipelines:
    - main_probelit.vert.glsl main_probelit_nossr.frag.glsl
    - main_probelit.vert.glsl main_probelit_nossr.frag.glsl
flags:
    - emissive
textures:
    - default_basecolor.tex
    - default_normalmap.dds
    - default_roughness.tex
params:
    - 1.0 0.0 0.0 0.5      # base color, roughness
    - 0.0 0.0 0.5 0.0      # sheen, sheen_tint, specular, spec_tint
    - 0.0 0.0 0.0 0.0      # metallic, transmission, clearcoat, clearcoat_roughness
    - 0.0 0.5 0.5 0.5      # transparency, emission color
